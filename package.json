    "build:assets": "node scripts/build_go_assets.js",
    "build:swagger": "npm-run-all -p clean:swagger build:swaggeri",
    "build:swaggeri": "node scripts/build_swagger.js",
    "clean:swagger": "rimraf src/swagger",
    "docs": "docsify serve docs",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "flow start": "flow start",
    "flow stop": "flow stop",
    "flow status": "flow status",
    "flow coverage": "flow coverage"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "verbose": true,
  "lint-staged": {
    "src/**/*.{js,jsx}": ["eslint --fix", "prettier --parser flow --write", "git add"],
    "src/**/*.css": ["stylefmt", "stylelint scss", "git add"],
    "src/**/*.scss": ["stylefmt", "stylelint --syntax scss", "git add"]
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:8088",
      "ws": true
    }
  }
